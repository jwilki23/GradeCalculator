@{
    ViewData["Title"] = "Jacob Wilkinson | Grade Calculator";
}
@{ 

}
@model GradeCalculatorModel;
<h2>Grade Calculator:</h2>

<form action="/" method="post">
    <label>Assignments: </label>
    <input asp-for="assignments" id="assignments" /><br />
    <label>Quizzes: </label>
    <input asp-for="quizzes" id="quizzes" /><br />
    <label>Group Projects: </label>
    <input asp-for="groupprojects" id="groupprojects" /><br />
    <label>INTEX: </label>
    <input asp-for="intex" id="intex" /><br />
    <label>Exams: </label>
    <input asp-for="exams" id="exams" /><br />
    <input type="submit" id="btnSend" value="Submit Grades" />
    @*<p id="output"></p>*@
    <div asp-validation-summary="All"></div>
</form>
<br />
<br />


<script src="js/GradeCalculator.js"></script>